<!DOCTYPE html>
<html lang="ko">
<head>
    <%- include('./adminHead'); %>   
</head>
<body>
    <%
    var keys = [], count = 0;
    for (fieldName in event[0]._doc) {
        if(fieldName !='_id' && !notAddDefault.includes(fieldName)) {
            keys[count++] = fieldName;
        }

    } 
    %>
    <main>
        <%- include('./sidebar') %>
        <div class="container">
            <div class="table-title">
                <span>Event Data</span>
                <button class="btn btn-primary add-new" data-toggle="modal" data-target="#addModal">
                    <i class="fa-solid fa-plus"></i>
                </button>
            </div>
            <div class="d-flex overflow-auto">
                <table class="table table-striped table-dark mt-5">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <% for (key of keys) { %>
                                <th scope="col"><%= key %></th>
                            <% } %>
                            <th scope="col">Delete</th>   
                        </tr>
                    </thead>
                    <tbody>
                        <% for (i in event) { %>
                        <tr>
                            <th scope="row"><%= i %></th>  
                            <% for (key of keys) { %>
                            <td nowrap>
                                <%= event[i][key] %>
                            </td>
                            <% } %>
                            <td>
                                <button type="button" class="delete btn btn-danger">Delete</button>
                            </td>
                        </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </main>
    <!-- <div class="container h-100"> -->
        <%- //include('../header'); %>
    <!-- </div> -->
    <%- include('../script'); %>
    <script>$('.nav-item:nth-child(3) > .nav-link').addClass('active');</script>
    <script src="/js/admin.js"></script>
</body>
</html>